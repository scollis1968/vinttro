{
  "name": "salesagility/suitecrm",
  "description": "SuiteCRM Application",
  "homepage": "https://suitecrm.com",
  "type": "project",
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "SalesAgility Ltd"
    }
  ],
  "config": {
    "vendor-dir": "vendor",
    "platform": {
      "php": "8.1.0"
    },
    "optimize-autoloader": true,
    "sort-packages": true,
    "allow-plugins": {
      "php-http/discovery": true,
      "symfony/flex": true,
      "symfony/runtime": true,
      "wikimedia/composer-merge-plugin": false
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "suggest": {
    "ext-imap": "Needed for emails module"
  },
  "require": {
    "php": ">=8.1",
    "ext-ctype": "*",
    "ext-curl": "*",
    "ext-gd": "*",
    "ext-iconv": "*",
    "ext-json": "*",
    "ext-openssl": "*",
    "ext-pdo": "*",
    "ext-zip": "*",
    "api-platform/core": "3.4.17",
    "composer/package-versions-deprecated": "1.11.99.3",
    "doctrine/doctrine-bundle": "^2.11",
    "doctrine/doctrine-migrations-bundle": "^3.3",
    "doctrine/migrations": "^3.2",
    "doctrine/orm": "^2.17",
    "elasticsearch/elasticsearch": "^7.13",
    "endroid/qr-code": "^5.0",
    "ezyang/htmlpurifier": "^4.10",
    "google/apiclient": "^2.7",
    "google/recaptcha": "^1.1",
    "gymadarasz/ace": "^1.2",
    "gymadarasz/imagesloaded": "^4.1",
    "javanile/php-imap2": "^0.1.10",
    "jeroendesloovere/vcard": "^1.7",
    "justinrainbow/json-schema": "^5.2",
    "league/oauth2-client": "^2.6",
    "league/oauth2-server": "^8.4",
    "monolog/monolog": "^1.23",
    "nbgrp/onelogin-saml-bundle": "*",
    "nelmio/cors-bundle": "^2.1",
    "nesbot/carbon": "2.72.6",
    "onelogin/php-saml": "^4",
    "paragonie/random_compat": "^2.0",
    "phpdocumentor/reflection-docblock": "^5.2",
    "phpmailer/phpmailer": "^6.0",
    "phpstan/phpdoc-parser": "^1.24",
    "psr/container": "^1.0",
    "psr/log": "^1.0",
    "scheb/2fa-backup-code": "^6.12",
    "scheb/2fa-bundle": "^6.12",
    "scheb/2fa-google-authenticator": "^6.12",
    "scheb/2fa-totp": "^6.12",
    "shivas/versioning-bundle": "^4.0",
    "slim/slim": "^3.8",
    "smarty/smarty": "^4",
    "soundasleep/html2text": "~0.5",
    "symfony/asset": "6.4.*",
    "symfony/console": "6.4.*",
    "symfony/doctrine-messenger": "6.4.*",
    "symfony/dotenv": "6.4.*",
    "symfony/expression-language": "6.4.*",
    "symfony/flex": "^2",
    "symfony/form": "6.4.*",
    "symfony/framework-bundle": "6.4.*",
    "symfony/http-client": "6.4.15",
    "symfony/http-foundation": "6.4.14",
    "symfony/intl": "6.4.*",
    "symfony/ldap": "6.4.*",
    "symfony/lock": "6.4.*",
    "symfony/mailer": "6.4.*",
    "symfony/mime": "6.4.*",
    "symfony/monolog-bundle": "^3.0",
    "symfony/notifier": "6.4.*",
    "symfony/options-resolver": "6.4.*",
    "symfony/process": "6.4.14",
    "symfony/property-access": "6.4.*",
    "symfony/property-info": "6.4.*",
    "symfony/proxy-manager-bridge": "6.4.*",
    "symfony/rate-limiter": "6.4.*",
    "symfony/runtime": "6.4.14",
    "symfony/security-bundle": "6.4.10",
    "symfony/security-csrf": "6.4.*",
    "symfony/security-http": "6.4.15",
    "symfony/serializer": "6.4.*",
    "symfony/string": "6.4.*",
    "symfony/translation": "6.4.*",
    "symfony/twig-bundle": "6.4.*",
    "symfony/validator": "6.4.11",
    "symfony/web-link": "6.4.*",
    "symfony/yaml": "6.4.*",
    "tecnickcom/tcpdf": "6.8.0",
    "tedivm/jshrink": "^1.3",
    "tinymce/tinymce": "^5.10",
    "twig/extra-bundle": "^2.12|^3.0",
    "twig/twig": "3.11.2",
    "vlucas/phpdotenv": "^3.5",
    "voku/anti-xss": "^4.1",
    "webonyx/graphql-php": "^15.0",
    "wikimedia/composer-merge-plugin": "^2.0",
    "zbateson/mail-mime-parser": "^2.2",
    "zf1/zend-gdata": "^1.12",
    "zf1/zend-loader": "^1.12",
    "zf1/zend-oauth": "^1.12",
    "zf1/zend-registry": "^1.12"
  },
  "require-dev": {
    "api-platform/schema-generator": "^5.2",
    "browserstack/browserstack-local": "^1.1",
    "codeception/codeception": "^5.0",
    "codeception/module-asserts": "^3.0",
    "codeception/module-filesystem": "^3.0",
    "codeception/module-phpbrowser": "^3.0",
    "codeception/module-rest": "^3.3",
    "codeception/module-symfony": "^3.2",
    "codeception/module-webdriver": "^3.2",
    "consolidation/robo": "^4.0",
    "dama/doctrine-test-bundle": "^8.0",
    "doctrine/doctrine-fixtures-bundle": "~3.5",
    "fakerphp/faker": "^1.14",
    "filp/whoops": "^2.15",
    "friendsofphp/php-cs-fixer": "^3.45",
    "mikey179/vfsstream": "1.6.*",
    "mockery/mockery": "^1.1.0",
    "phpunit/phpunit": "^9.5",
    "scssphp/scssphp": "^1.5",
    "symfony/browser-kit": "6.4.*",
    "symfony/css-selector": "6.4.*",
    "symfony/debug-bundle": "6.4.*",
    "symfony/phpunit-bridge": "^7.0",
    "symfony/maker-bundle": "^1.14",
    "symfony/stopwatch": "6.4.*",
    "symfony/web-profiler-bundle": "6.4.*"
  },
  "autoload": {
    "files": [
      "public/legacy/deprecated.php"
    ],
    "psr-4": {
      "App\\": "core/backend/",
      "App\\Module\\": "core/modules/",
      "App\\Extension\\": "extensions/",
      "SuiteCRM\\": [
        "public/legacy/lib/",
        "public/legacy/include/"
      ],
      "SuiteCRM\\Custom\\": [
        "public/legacy/custom/lib"
      ],
      "SuiteCRM\\Modules\\": [
        "public/legacy/modules/"
      ]
    },
    "classmap": [
      "public/legacy/Api/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/",
      "SuiteCRM\\": [
        "public/legacy/tests/SuiteCRM/",
        "public/legacy/tests/unit/phpunit/lib/SuiteCRM/"
      ],
      "SuiteCRM\\Tests\\Unit\\": [
        "public/legacy/tests/unit/phpunit/"
      ]
    }
  },
  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php73": "*",
    "symfony/polyfill-php74": "*",
    "symfony/polyfill-php80": "*",
    "symfony/polyfill-php81": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "cache:warmup": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd",
      "scrm:copy-legacy-assets": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts",
      "Google\\Task\\Composer::cleanup"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "src-dir": "core/backend",
    "symfony": {
      "allow-contrib": false,
      "require": "6.4.*"
    },
    "google/apiclient-services": [
      "Calendar"
    ],
    "runtime": {
      "autoload_template": "core/backend/Composer/Runtime/autoload_runtime.template"
    },
    "merge-plugin": {
      "include": [
        "public/legacy/composer.ext.json",
        "composer.ext.json",
        "public/legacy/custom/Extension/application/Ext/Composer/*/*.json"
      ],
      "recurse": true,
      "replace": false,
      "ignore-duplicates": false,
      "merge-dev": true,
      "merge-extra": false,
      "merge-extra-deep": false,
      "merge-scripts": false
    }
  },
  "bin": [
    "bin/console"
  ]
}
